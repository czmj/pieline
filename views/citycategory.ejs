<div class="list-group col-xs-12 col-lg-7">
        <a href="https://www.bytemark.co.uk/company/employment/" class="list-group-item list-group-item-success">Sponsored link: Bytemark Hosting are hiring in Manchester</a>

                <%  var title
                 	for(var i = 0;i < data.length;i++) { 
                        title=data[i].title.replace(/[&\/\\#,+()$~%.'":*?<>{}]/g,'-');
                        company=data[i].company
                        location=data[i].location
                        title=title.replace(/\s+/g, '-');
                        location=location.replace(/\s+/g, '-');
                        company=company.replace(/\s+/g, '-');
			 %>
<a href="/jobs/<%=location%>/<%=data[i].id%>/<%=title%>-at-<%=company%>" class="list-group-item">   
                    <%=data[i].title%>
                    at <%=data[i].company%>
                </a>
			<%}%>

</div>

